{
    "name": "atkinshealth/silverstripe-autodoc",
    "description": "Automatically generate PHPDoc comments for DataObject subclasses based on fields and relations.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "ORM"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7 | ^8",
        "silverstripe/framework": "^4.0 || ^5",
        "atanamo/php-codeshift": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AtkinsHealth\\AutoDoc\\": "src/",
            "AtkinsHealth\\AutoDoc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
